module DecreasingScalar(
	input clk,
	input reset_n, // !note_on
	input [4:0]initial_value,
	output [4:0]scalar_out
);

wire [4:0]count;
wire [4:0]scale_compare;

// will hold value at reset
BinaryCountdown_4bit counter(
	clk,
	reset_n,
	count
);

assign scalar_out = count;


endmodule